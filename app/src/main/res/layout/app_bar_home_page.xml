<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:showIn="@layout/activity_home_page">

        <include layout="@layout/toolbar"
            android:id="@+id/toolbar"/>

    <include
        android:id="@+id/content_home_page"
        layout="@layout/content_home_page"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:layout_marginBottom="56dp"
        android:layout_below="@+id/toolbar" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/floatingActionButton1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/content_home_page"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="13dp"
        android:clickable="true"
        android:focusable="true"
        app:srcCompat="@drawable/tw__composer_logo_white" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/floatingActionButton3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/floatingActionButton1"
        android:clickable="true"
        android:focusable="true"
        app:srcCompat="@drawable/settings" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/floatingActionButton2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/floatingActionButton1"
        android:layout_alignParentEnd="true"
        android:clickable="true"
        android:focusable="true"
        app:srcCompat="@drawable/search"
        tools:ignore="VectorDrawableCompat" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/floatingActionButtonMenu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/floatingActionButton2"
        android:clickable="true"
        android:focusable="true"
        app:srcCompat="@drawable/information"/>
</RelativeLayout>
